{
  "name": "wavekb",
  "version": "1.0.0",
  "description": "A Knowledge Base that lets you find gaps in the knowledge!",
  "main": "index.js",
  "scripts": {
    "postinstall": "npm-run-all postinstall:patch postinstall:install",
    "postinstall:patch": "patch-package",
    "postinstall:install": "npm install --prefix app",
    "test": "echo \"Error: no test specified\" && exit 1",
    "synth": "cdk synth",
    "diagram": "npm-run-all synth digram:generate",
    "digram:generate": "cdk-dia",
    "dev:up": "docker-compose -f docker/docker-compose.yml up",
    "dev:down": "docker-compose -f docker/docker-compose.yml down"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/3p3r/wavekb.git"
  },
  "keywords": [
    "RAG"
  ],
  "author": "Sepehr Laal",
  "license": "MIT",
  "type": "commonjs",
  "bugs": {
    "url": "https://github.com/3p3r/wavekb/issues"
  },
  "homepage": "https://github.com/3p3r/wavekb#readme",
  "devDependencies": {
    "@types/node": "^24.3.0",
    "aws-cdk": "^2.1027.0",
    "aws-cdk-lib": "^2.213.0",
    "cdk-dia": "^0.12.0",
    "cdk-nextjs-standalone": "^4.3.0",
    "constructs": "^10.4.2",
    "docker-compose-cdk": "^0.0.3",
    "npm-run-all": "^4.1.5",
    "patch-package": "^8.0.0",
    "source-map-support": "^0.5.21",
    "tsx": "^4.20.5",
    "yaml": "^2.8.1"
  }
}
